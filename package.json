{
  "name": "vite-todo",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:script:fix": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:style:fix": "stylelint src/**/*.{css,scss,vue} --fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.1",
    "@heroicons/vue": "^1.0.5",
    "@vueuse/core": "^6.8.0",
    "path": "^0.12.7",
    "pinia": "^2.0.2",
    "vue": "^3.2.16",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.0",
    "eslint": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss-html": "^1.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-windicss": "^1.4.12",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "yarn run lint:script:fix",
    "*.{css,scss,vue}": "yarn run lint:style:fix",
    "*": "prettier -w -u"
  }
}
